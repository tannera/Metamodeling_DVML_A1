<?xml version="1.0" encoding="UTF-8"?>
<views:DomainModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:views="http://www.example.org/views"
    xsi:schemaLocation="http://www.example.org/views ../model/views.ecore">
  <class name="Institute">
    <property
        name="number"
        LowerBound="1"
        class="//@class.0"
        isID="true"/>
    <property
        name="name"
        LowerBound="1"
        class="//@class.0"/>
    <association
        name="professors">
      <associationend
          name="Institute"
          class="//@class.0"/>
      <associationend
          name="professors"
          navigable="true"
          LowerBound="1"/>
    </association>
  </class>
  <class name="Course">
    <property
        name="type"
        type="Enumeration"
        LowerBound="1"
        class="//@class.1"/>
    <property
        name="title"
        type="DataType"
        LowerBound="1"
        class="//@class.1"
        isID="true"/>
    <property
        name="credits"
        type="DataType"
        class="//@class.1"/>
    <property
        name="description"
        type="DataType"
        class="//@class.1"/>
    <property
        name="date"
        type="DataType"
        class="//@class.1"/>
    <property
        name="time"
        type="DataType"
        class="//@class.1"/>
    <property
        name="topic"
        type="DataType"
        class="//@class.1"/>
    <property
        name="keywords"
        type="DataType"
        class="//@class.1"/>
    <association
        name="courses">
      <associationend
          name="institute"
          class="//@class.0"/>
      <associationend
          name="courses"
          class="//@class.1"
          navigable="true"
          Composed="true"
          UpperBound="-1"/>
    </association>
  </class>
  <class name="Staff">
    <property
        name="firstname"
        type="DataType"
        LowerBound="1"
        class="//@class.2"/>
    <property
        name="lastname"
        type="DataType"
        LowerBound="1"
        class="//@class.2"/>
    <property
        name="email"
        type="DataType"
        LowerBound="1"
        class="//@class.2"
        isID="true"/>
  </class>
  <class name="Professor"
      superclass="//@class.2">
    <property
        name="visitingProfessor"
        type="DataType"
        LowerBound="1"
        class="//@class.3"/>
    <property
        name="homeUniversity"
        type="DataType"
        class="//@class.3"/>
  </class>
  <datatype
      name="Integer"/>
  <datatype
      name="Double"/>
  <datatype
      name="String"/>
  <datatype
      name="Boolean"/>
  <datatype
      name="DateTime"/>
  <enumeration
      name="TeachingType">
    <literal
        name="Lecture"
        value="LE"/>
    <literal
        name="Thesis"
        value="TH"/>
  </enumeration>
</views:DomainModel>
